{
  "name": "nitsan/ns-facebook-comment",
  "type": "typo3-cms-extension",
  "description": "Grab this extension which will help you to add facebook comment plugin to add and share comments, reply, listing etc.",
  "homepage": "https://t3terminal.com/typo3-facebook-comment-free/",
  "license": "GPL-2.0+",
  "keywords": [
    "TYPO3 CMS",
    "facebook",
    "App ID",
    "NITSAN",
    "comments",
    "ns_facebook_comment"
  ],
  "support": {
    "issues": "https://github.com/nitsan-technologies/ns_facebook_comment/issues"
  },
  "require": {
     "typo3/cms-core": "^6.0 || ^7.0 || ^8.0  || ^9.0 || ^10.0  || ^11.0 "
  },
  "autoload": {
    "psr-4": {
      "Nitsan\\NsFacebookComment\\": "Classes/"
    }
  },
  "replace": {
      "typo3-ter/ns-facebook-comment": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor/",
    "bin-dir": ".Build/bin/"
  },
  "extra": {
    "typo3/cms": {
        "cms-package-dir": "{$vendor-dir}/typo3/cms",
        "web-dir": ".Build/public",
        "extension-key": "ns_facebook_comment"
    }
  }
}
